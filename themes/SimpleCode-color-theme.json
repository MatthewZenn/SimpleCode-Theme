		"tab.border": "#1a1a1a",
		"settings.dropdownBackground": "#1d1d1d",
		"settings.dropdownBorder": "#aaaaaa",
		"sideBarSectionHeader.background": "#1a1a1a",
		"menu.background": "#1a1a1a",
		"menu.selectionBackground": "#ff604b",
		"dropdown.background": "#141414",
		"dropdown.listBackground": "#141414",
		"list.inactiveSelectionBackground": "#1a1a1a",
		"editorGroupHeader.noTabsBackground": "#242f35",
		"editorGroupHeader.tabsBackground": "#1a1a1a",
		"editorHoverWidget.background": "#1d1d1d",
		"editorHoverWidget.border": "#aaaaaa",
		"editorBracketMatch.border": "#ff604b",
		"editorSuggestWidget.background": "#1d1d1d",
		"editorSuggestWidget.border": "#aaaaaa",
		"notifications.background": "#1d1d1d",
		"notificationCenterHeader.background": "#2a383f",
		"badge.background": "#ff604b",
		"statusBar.background": "#1a1a1a",
		"button.background": "#ff604b",
		"quickInput.background": "#141414",
		"list.hoverBackground": "#1a1a1a",
		"keybindingLabel.background": "#1d1d1d",
		"editorLink.activeForeground": "#89DDFF",
		"symbolIcon.classForeground": "#ff8e7a",
		"symbolIcon.fieldForeground": "#89DDFF",
		"symbolIcon.variableForeground": "#89DDFF",
		"symbolIcon.functionForeground": "#dda3ff",
