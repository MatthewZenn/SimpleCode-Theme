			"settings": {
				"foreground": "#ff7a89"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#dda3ff"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
