{
	"name": "SimpleCode",
	"type": "dark",
	"colors": {
		"editor.background": "#1a1a1a",
		"editor.foreground": "#ffffff",
		"activityBarBadge.background": "#ff604b",
		"sideBarTitle.foreground": "#ffffff",
